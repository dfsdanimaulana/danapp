    <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css"
    />

    <!-- My css -->
    <link href="css/main.css" rel="stylesheet" />
    <link href="css/<%= style %>.css" rel="stylesheet" />

    <!-- my script -->
    <script src="js/<%= script %>.js" defer></script>

    <title><%= title %></title>
