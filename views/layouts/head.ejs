    <meta name="theme-color" content="rgb(1,43,65)" />
    <!-- css reset -->
    <link href="css/reset.css" rel="stylesheet" />

    <!-- gsap js -->
    <script
        src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.7.0/gsap.min.js"
