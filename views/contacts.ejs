<!-- header -->
<div class="contacts-container">
    <div class="contacts-item-navbar">
        <a href="/chat"><i class="bi bi-arrow-left"></i></a>
        <h1>Select Contact</h1>
        <a href="#"><i class="bi bi-search"></i></a>
        <a href="/signup"><i class="bi bi-three-dots-vertical"></i></a>
    </div>
</div>
<!-- header end -->

<% data.forEach(list => { %>
    <div class="contact-profile">
        <a href="/profile/<%= list._id %>">
            <img src="/<%= list.img %>" alt="not found"></img>
        </a>
    <p>
        <%= list.name %>
    </p>
    <i class="bi bi-dot"></i>
    <p>
        <%= list.status %>
    </p>
    <p>
        <%= list.timeSend %>
    </p>
    <a href="/chatroom/<%= list._id %>/<%= list.name %>">
        <i class="bi bi-chat"></i>
    </a>
    <i class="bi bi-pencil-square"></i>
    </div>
<% }) %>